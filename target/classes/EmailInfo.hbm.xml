<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="XmlGenerator.EmailInfo" table="emailinfo">
        <id name="id" type="int" column="id">
            <generator class="native"/>
        </id>
        <property name="login" column="Login" type="string"/>
        <property name="password" column="Password" type="string"/>
        <property name="repeat_password" column="Repeat_pass" type="string"/>
        <property name="name" column="Name" type="string"/>
        <property name="surname" column="Surname" type="string"/>
        <property name="fullname" column="Fullname" type="string"/>
        <property name="city_postalcode" column="City_code" type="string"/>
        <property name="alternative_contact" column="Alternative_Contact" type="string"/>
        <property name="birthDate_day" column="Birth_Day" type="string"/>
        <property name="birthDate_month" column="Birth_Month" type="string"/>
        <property name="birthDate_year" column="Birth_Year" type="string"/>
    </class>
</hibernate-mapping>